{
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@cloudflare/vitest-pool-workers": "^0.12.12",
    "@trunkio/launcher": "^1.3.4",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  },
  "engines": {
    "bun": "^1.3.9",
    "node": "^24.13.1"
  },
  "name": "dcw-soy",
  "packageManager": "bun@1.3.9",
  "private": true,
  "scripts": {
    "cf-typegen": "bun run wrangler types",
    "deploy": "bun run wrangler deploy",
    "deploy:nonprod": "bun run wrangler versions upload",
    "dev": "bun run wrangler dev",
    "format:prettier": "bun run prettier --write .",
    "format:trunk": "bun run trunk fmt -a",
    "lint": "bun run-s lint:biome lint:trunk lint:types",
    "lint:biome": "bun run biome check .",
    "lint:fix": "bun run-s lint:fix:biome lint:fix:trunk",
    "lint:fix:biome": "bun run biome check --fix --unsafe .",
    "lint:fix:trunk": "bun run trunk check -a --fix",
    "lint:format": "bun run-s lint:format:prettier lint:format:biome lint:format:trunk",
    "lint:format:biome": "bun run biome format --write .",
    "lint:format:prettier": "bun run prettier --write .",
    "lint:format:trunk": "trunk fmt -a",
    "lint:trunk": "bun run trunk check -a --fix",
    "lint:types": "tsc --noEmit",
    "test": "bun run vitest run",
    "types": "bun run postinstall",
    "postinstall": "bun run-s cf-typegen lint:format"
  },
  "version": "0.1.0",
  "trustedDependencies": [
    "protobufjs",
    "tree-sitter-bash"
  ]
}
