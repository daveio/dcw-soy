{
  "$schema": "node_modules/wrangler/config-schema.json",
  "assets": {
    "binding": "ASSETS",
    "directory": "./public/",
  },
  "compatibility_date": "2025-07-29",
  "compatibility_flags": ["global_fetch_strictly_public"],
  "main": "src/index.ts",
  "name": "dcw-soy",
  "observability": {
    "enabled": true,
  },
  "placement": {
    "mode": "smart",
  },
  "kv_namespaces": [
    {
      "binding": "REDIRECT_CACHE",
      "id": "REDIRECT_CACHE",
    },
  ],
}
